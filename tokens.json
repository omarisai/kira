{
    "filenames": {
        "start": "@",
        "end": "#",
        "separator": "-",
        "iterator": "$",
        "sub": "&"
    },
    "*.jsonc": {
        "target": ".json",
        "keyword": {
            "start": "@",
            "end": "#",
            "separator": ".",
            "escape": "$",
            "sub": "&"
        },
        "block": {
            "start": "//@",
            "end": "//#",
            "iterator": ":"
        }
    }
}